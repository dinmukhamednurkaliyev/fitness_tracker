{
  "search.exclude": {
    "**/.fvm": true,
    "**/.git": true,
    "**/.dart_tool": true,
    "**/build": true,
    "**/.pub-cache": true,
    "**/ios/Pods": true,
    "**/*.g.dart": true,
    "**/*.freezed.dart": true,
    "**/*.config.dart": true
  },
  "files.watcherExclude": {
    "**/*.freezed.dart": true,
    "**/*.g.dart": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.dart_tool/**": true,
    "**/build/**": true,
    "**/.pub-cache/**": true,
    "**/ios/Pods/**": true
  },
  "explorer.fileNesting.patterns": {
    "pubspec.yaml": "pubspec.lock, pubspec_overrides.yaml, .packages",
    "*.dart": "${capture}.freezed.dart, ${capture}.g.dart, ${capture}.gr.dart",
    ".env": ".env.example, .env.local"
  },
}
